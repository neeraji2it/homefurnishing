<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

    <!-- Viewport metatags -->
    <meta name="HandheldFriendly" content="true" />
    <meta name="MobileOptimized" content="320" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />

    <!-- iOS webapp metatags -->
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />

    <!-- iOS webapp icons -->
    <link rel="apple-touch-icon" href="touch-icon-iphone.png" />
    <link rel="apple-touch-icon" sizes="72x72" href="touch-icon-ipad.png" />
    <link rel="apple-touch-icon" sizes="114x114" href="touch-icon-retina.png" />
    <title>RosaraHomes Admin</title>
    <!-- CSS Reset -->
    <%#*<link rel="stylesheet" type="text/css" href="/assets/admin/css/reset.css" media="screen" />%>
    <!--  Fluid Grid System -->
    <%#*<link rel="stylesheet" type="text/css" href="/assets/admin/css/fluid.css" media="screen" />%>
    <!-- Theme Stylesheet -->
    <%#*<link rel="stylesheet" type="text/css" href="/assets/admin/css/dandelion.theme.css" media="screen" />%>
    <!--  Main Stylesheet -->
    <%#*<link rel="stylesheet" type="text/css" href="/assets/admin/css/dandelion.css" media="screen" />%>
    <!-- Demo Stylesheet -->
    <%#*<link rel="stylesheet" type="text/css" href="/assets/admin/css/demo.css" media="screen" />%>
    <%= stylesheet_link_tag "application", "admin/css/reset.css", "admin/css/fluid.css",
  "admin/css/dandelion.theme.css", "admin/css/dandelion.css", "admin/css/demo.css", "admin/jui/css/jquery.ui.all.css", "admin/plugins/tipsy/tipsy.css", :media => "all" %>
    <%= javascript_include_tag "application", "admin/js/jquery-1.7.2.min.js", "admin/jui/js/jquery-ui-1.8.20.min.js", "admin/jui/js/jquery.ui.timepicker.min.js", "admin/jui/js/jquery.ui.touch-punch.min.js", "admin/js/jquery.fileinput.js", "admin/js/jquery.placeholder.js", "admin/js/jquery.mousewheel.min.js", "admin/js/jquery.tinyscrollbar.min.js", "admin/plugins/tipsy/jquery.tipsy-min.js", "admin/js/jquery.metadata.js", "admin/js/core/plugins/dandelion.circularstat.min.js", "admin/js/demo/demo.statistic.js", "admin/js/core/dandelion.core.js", "admin/js/core/dandelion.customizer.js" %>
    
    <!-- jQuery JavaScript File -->
    <%#*<script type="text/javascript" src="/assets/admin/js/jquery-1.7.2.min.js"></script>%>

    <!-- jQuery-UI JavaScript Files -->
    <%#*<script type="text/javascript" src="/assets/admin/jui/js/jquery-ui-1.8.20.min.js"></script>%>
    <%#*<script type="text/javascript" src="/assets/admin/jui/js/jquery.ui.timepicker.min.js"></script>%>
    <%#*<script type="text/javascript" src="/assets/admin/jui/js/jquery.ui.touch-punch.min.js"></script>%>
    <%#*<link rel="stylesheet" type="text/css" href="/assets/admin/jui/css/jquery.ui.all.css" media="screen" />%>

    <!-- Plugin Files -->

    <!-- FileInput Plugin -->
    <%#*<script type="text/javascript" src="/assets/admin/js/jquery.fileinput.js"></script>%>
    <!-- Placeholder Plugin -->
    <%#*<script type="text/javascript" src="/assets/admin/js/jquery.placeholder.js"></script>%>
    <!-- Mousewheel Plugin -->
    <%#*<script type="text/javascript" src="/assets/admin/js/jquery.mousewheel.min.js"></script>%>
    <!-- Scrollbar Plugin -->
    <%#*<script type="text/javascript" src="/assets/admin/js/jquery.tinyscrollbar.min.js"></script>%>
    <!-- Tooltips Plugin -->
    <%#*<script type="text/javascript" src="/assets/admin/plugins/tipsy/jquery.tipsy-min.js"></script>%>
    <%#*<link rel="stylesheet" href="/assets/admin/plugins/tipsy/tipsy.css" />%>

    <!-- Statistic Plugin JavaScript Files (requires metadata and excanvas for IE) -->
    <%#*<script type="text/javascript" src="/assets/admin/js/jquery.metadata.js"></script>%>
    <!--[if lt IE 9]>
    <script type="text/javascript" src="/assets/admin/js/excanvas.js"></script>
    <![endif]-->
    <%#*<script type="text/javascript" src="/assets/admin/js/core/plugins/dandelion.circularstat.min.js"></script>%>

    <!-- Demo JavaScript Files -->
    <%#*<script type="text/javascript" src="/assets/admin/js/demo/demo.statistic.js"></script>%>

    <!-- Core JavaScript Files -->
    <%#*<script type="text/javascript" src="/assets/admin/js/core/dandelion.core.js"></script>%>

    <!-- Customizer JavaScript File (remove if not needed) -->
    <%#*<script type="text/javascript" src="/assets/admin/js/core/dandelion.customizer.js"></script>%>

    <%#= stylesheet_link_tag "application", "bootstrap.min", "style",  :media => "all" %>
    <%#= javascript_include_tag "application", "js/bootstrap/bootstrap.min" %>
    <%= csrf_meta_tags %>
  </head>

  <body>

    <!-- Dandelion Customizer (remove if not needed) -->
      
    <!-- Main Wrapper. Set this to 'fixed' for fixed layout and 'fluid' for fluid layout' -->
    <div id="da-wrapper" class="fluid">

      <!-- Header -->
      <div id="da-header">

        <div id="da-header-top">

          <!-- Container -->
          <%= render :partial => "layouts/admin_header" %>
        </div>

        <div id="da-header-bottom">
          <!-- Container -->
          <div class="da-container clearfix">
            
            <!-- Breadcrumbs -->
            <div id="da-breadcrumb">
              <ul>
                <li><a href="#"><img src="/assets/admin/images/icons/black/16/home.png" alt="Home" />Dashboard</a></li>
                <li><a href="#">Statistics and Charts</a></li>
                <li class="active"><span>Statistic Widgets</span></li>
              </ul>
            </div>

          </div>
        </div>
      </div>

      <!-- Content -->
      <div id="da-content">

        <!-- Container -->
        <div class="da-container clearfix">

          <!-- Sidebar -->
          <div id="da-sidebar-separator"></div>
          <%= render :partial => "layouts/admin_sidebar" %>

          <!-- Main Content Wrapper -->
          <div id="da-content-wrap" class="clearfix">
            <%= yield %>
          </div>

        </div>

      </div>

      <!-- Footer -->
      <div id="da-footer">
        <div class="da-container clearfix">
          <p>Copyright 2014. RosaraHomes Admin. All Rights Reserved.
        </div>
      </div>

    </div>

  </body>
</html>